{
  "installCommand": "pnpm install && pip install mkdocs-material",
  "buildCommand": "pnpm build && cd ../../docs && mkdocs build --site-dir ../packages/frontend/dist/docs",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "http://34.170.58.101/api/:path*"
    },
    {
      "source": "/docs/:path*",
      "destination": "/docs/:path*"
    },
    {
      "source": "/((?!assets|docs).*)",
      "destination": "/index.html"
    }
  ]
}
